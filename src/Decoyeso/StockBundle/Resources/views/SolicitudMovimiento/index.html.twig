<h1>SolicitudMovimiento list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
           
            <th>Estado</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('solicitudmovimiento_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.fechaHoraCreado %}{{ entity.fechaHoraCreado|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{% if entity.fechaHoraReserva %}{{ entity.fechaHoraReserva|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{% if entity.fechaHoraCierre %}{{ entity.fechaHoraCierre|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{{ entity.estado }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('solicitudmovimiento_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('solicitudmovimiento_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('solicitudmovimiento_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
