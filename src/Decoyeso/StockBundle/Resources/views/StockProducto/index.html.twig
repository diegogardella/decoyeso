<h1>StockProducto list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Accion</th>
            <th>Motivo</th>
            <th>Observacion</th>
            <th>Cantidad</th>
            <th>Fechahora</th>
            <th>Cantidadtotal</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('stockproducto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.accion }}</td>
            <td>{{ entity.motivo }}</td>
            <td>{{ entity.observacion }}</td>
            <td>{{ entity.cantidad }}</td>
            <td>{% if entity.fechaHora %}{{ entity.fechaHora|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{{ entity.cantidadTotal }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('stockproducto_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('stockproducto_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('stockproducto_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
