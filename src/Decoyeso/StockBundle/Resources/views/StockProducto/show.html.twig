<h1>StockProducto</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Accion</th>
            <td>{{ entity.accion }}</td>
        </tr>
        <tr>
            <th>Motivo</th>
            <td>{{ entity.motivo }}</td>
        </tr>
        <tr>
            <th>Observacion</th>
            <td>{{ entity.observacion }}</td>
        </tr>
        <tr>
            <th>Cantidad</th>
            <td>{{ entity.cantidad }}</td>
        </tr>
        <tr>
            <th>Fechahora</th>
            <td>{{ entity.fechaHora|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Cantidadtotal</th>
            <td>{{ entity.cantidadTotal }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('stockproducto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('stockproducto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('stockproducto_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
