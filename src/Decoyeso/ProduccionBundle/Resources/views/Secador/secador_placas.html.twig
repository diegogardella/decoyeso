		<div class="NSecadorCont itemSecadorPlaca">
			<table style="width: 90%;">
				<tr>
					<td>
						<span class="txt3">Secador:</span> <span class="Ntxt3">{{ sP.nombre }}</span>
						<a href="{{ path('secador_vaciar', { 'id': sP.id }) }}" class="NVaciarSecador btn_red_button ">Vaciar Secador</a>
					
					</td>
				</tr>
				<tr>
					<td><span class="txt3">Capacidad:</span> <span class="Ntxt3">{{ sP.capacidad }} Un.</span></td>
		
				</tr>
				<tr>
					<td></td>
				</tr>
			</table>
			
		
			
			
			 
			<div class="fix"><br></div>
		
			<div class="NSecador">
				{% for lugar in sP.lugaresSecador %}
					
					{% if lugar.getDisponible() == 0 %}
						<div class="NLugar" id="{{ loop.index }}">
					{% endif %}
					{% if lugar.getDisponible() == 1 %}
						<div class="NLugar NColor3 " id="{{ loop.index }}">
					{% endif %}
					
						</div>
						
					{% if (loop.index % 45 == 0) %}
					<div class="NLugarSep"></div>
					{% endif %}
					{% if (loop.index == 90) %}
					<div class="fix"></div>
					{% endif %}
						
				{% endfor %}
			</div>
		
		</div>
		
		<div class="fix"><br></div>